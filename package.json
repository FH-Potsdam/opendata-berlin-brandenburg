{
  "name": "opendata-berlin-brandenburg",
  "version": "1.0.0",
  "description": "open data tutorial site for Berlin & Brandenburg",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=prod eleventy",
    "seed": "cross-env ELEVENTY_ENV=seed eleventy",
    "serve": "http-server ./dist",
    "backend": "node ./dev/editor.js"
  },
  "engines": {
    "npm": ">=9.8.1",
    "node": ">=18.12.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FH-Potsdam/opendata-berlin-brandenburg.git"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "cssnano": "^6.0.1",
    "eleventy-plugin-toc": "^1.1.5",
    "moment": "^2.29.4",
    "postcss": "^8.4.27",
    "postcss-color-mix": "^1.1.0",
    "postcss-import": "^15.1.0",
    "postcss-mixins": "^9.0.4",
    "postcss-normalize": "^10.0.1",
    "precss": "^4.0.0",
    "uglify-js": "^3.17.4"
  },
  "author": "sebastian-meier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FH-Potsdam/opendata-berlin-brandenburg/issues"
  },
  "homepage": "https://github.com/FH-Potsdam/opendata-berlin-brandenburg#readme",
  "devDependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "http-server": "^14.1.1"
  }
}
